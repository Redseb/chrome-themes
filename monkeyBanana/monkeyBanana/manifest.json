{
    "manifest_version": 2,
    "name": "Monkey Banana",
    "description": "Overrides the new tab page",
    "version": "0.0.1",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "permissions": [],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_action": {
        "default_icon": "images/icon48.png"
    }
}